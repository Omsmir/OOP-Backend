{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "esnext",
    "externalHelpers": false,
    "keepClassNames": true,
    "loose": false,
    "minify": {
      "compress": false,
      "mangle": false
    },
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@config": ["src/config"],
      "@controllers/*": ["controllers/*"],
      "@exceptions/*": ["exceptions/*"],
      "@interfaces/*": ["interfaces/*"],
      "@middlewares/*": ["middlewares/*"],
      "@templates/*": ["templates/*"],
      "@models/*": ["models/*"],
      "@routes/*": ["routes/*"],
      "@schemas/*": ["schemas/*"],
      "@services/*": ["services/*"],
      "@utils/*": ["utils/*"],
      "@classes/*": ["classes/*"]
    }
  },
  "module": {
    "type": "commonjs"
  }
}
